{
	"$schema": "vscode://schemas/color-theme",
	"type": "dark",
	"colors": {
		"focusBorder": "#0000",
		"foreground": "#cccccc",
		"disabledForeground": "#636363",
		"widget.shadow": "#0000",
		"selection.background": "#121212",
		"descriptionForeground": "#636363",
		"errorForeground": "#fea9fd",
		"icon.foreground": "#636363",
		"sash.hoverBorder": "#03e6fd",
		"activityBar.background": "#262626",
		"editor.background": "#121212",
		"editor.foreground": "#cccccc",
		"editor.hoverHighlightBackground": "#000",
		"editor.lineHighlightBackground": "#000",
		"editor.lineHighlightBorder": "#0000",
		"editor.selectionBackground": "#2e2e2e",
		"editor.selectionHighlightBackground": "#2e2e2e",
		"editor.wordHighlightBackground": "#262626",
		"editor.wordHighlightBorder": "#0000",
		"editor.wordHighlightStrongBackground": "#262626",
		"editor.wordHighlightStrongBorder": "#2e2e2e",
		"editorBracketMatch.background": "#2e2e2e",
		"editorBracketMatch.border": "#0000",
		"editorCursor.foreground": "#fff",
		"editorLineNumber.activeForeground": "#fff",
		"editorLineNumber.foreground": "#636363",
		"list.activeSelectionBackground": "#121212",
		"list.activeSelectionForeground": "#fff",
		"list.inactiveSelectionBackground": "#121212",
		"list.inactiveSelectionForeground": "#fff",
		"sideBar.background": "#181818",
		"sideBarSectionHeader.background": "#262626"
	},
	"tokenColors": [
		{
			"scope": ["keyword", "punctuation", "storage"],
			"settings": {
				"foreground": "#636363"
			}
		},
		{
			"scope": ["string", "constant", "support.constant"],
			"settings": {
				"foreground": "#cccccc",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["support", "keyword.operator.new", "entity.name"],
			"settings": {
				"foreground": "#fea9fd"
			}
		},
		{
			"scope": ["comment"],
			"settings": {
				"foreground": "#03e6fd"
			}
		},
		{
			"scope": [
				"entity.name.type.alias",
				"entity.name.type.class",
				"entity.name.type.interface",
				"meta.definition.function"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#03e6fd"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#cd9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#f44747"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#fea9fd"
			}
		}
	]
}
