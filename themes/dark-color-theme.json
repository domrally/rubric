{
	"$schema": "vscode://schemas/color-theme",
	"type": "dark",
	"colors": {
		"activityBar.background": "#222222",
		"editor.background": "#151515",
		"editor.foreground": "#b4b4b4",
		"editorLineNumber.activeForeground": "#37e6ff",
		"editorLineNumber.foreground": "#636363",
		"focusBorder": "#00000000",
		"list.activeSelectionBackground": "#151515",
		"list.activeSelectionForeground": "#eeeeee",
		"list.inactiveSelectionBackground": "#151515",
		"list.inactiveSelectionForeground": "#eeeeee",
		"sideBar.background": "#1b1b1b",
		"sideBarSectionHeader.background": "#222222"
	},
	"tokenColors": [
		{
			"scope": ["keyword", "storage"],
			"settings": {
				"foreground": "#636363"
			}
		},
		{
			"scope": ["string", "constant"],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["support", "keyword.operator.new", "entity"],
			"settings": {
				"foreground": "#FEACFD"
			}
		},
		{
			"scope": ["comment"],
			"settings": {
				"foreground": "#37E6FF"
			}
		},
		{
			"scope": [
				"entity.name.type.alias",
				"entity.name.type.class",
				"entity.name.type.interface",
				"meta.definition.function"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6796E6"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#CD9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#F44747"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#B267E6"
			}
		}
	]
}
