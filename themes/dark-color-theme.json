{
	"$schema": "vscode://schemas/color-theme",
	"type": "dark",
	"colors": {
		"focusBorder": "#0000",
		"foreground": "#bbb",
		"disabledForeground": "#949494",
		"widget.shadow": "#0000",
		"selection.background": "#161616",
		"descriptionForeground": "#949494",
		"errorForeground": "#fc8dfd",
		"icon.foreground": "#949494",
		"sash.hoverBorder": "#00747a",
		"activityBar.background": "#161616",
		"editor.background": "#333",
		"editor.foreground": "#aeaeae",
		"editor.hoverHighlightBackground": "#000",
		"editor.lineHighlightBackground": "#000",
		"editor.lineHighlightBorder": "#0000",
		"editor.selectionBackground": "#000",
		"editor.selectionHighlightBackground": "#222222",
		"editor.wordHighlightBackground": "#222222",
		"editor.wordHighlightBorder": "#0000",
		"editor.wordHighlightStrongBackground": "#222222",
		"editor.wordHighlightStrongBorder": "#222222",
		"editorBracketMatch.background": "#222222",
		"editorBracketMatch.border": "#0000",
		"editorCursor.foreground": "#fff",
		"editorLineNumber.activeForeground": "#fff",
		"editorLineNumber.foreground": "#949494",
		"list.activeSelectionBackground": "#161616",
		"list.activeSelectionForeground": "#fff",
		"list.inactiveSelectionBackground": "#161616",
		"list.inactiveSelectionForeground": "#fff",
		"sideBar.background": "#161616",
		"sideBarSectionHeader.background": "#161616"
	},
	"tokenColors": [
		{
			"scope": ["keyword", "punctuation", "storage"],
			"settings": {
				"foreground": "#555"
			}
		},
		{
			"scope": ["string", "constant", "support.constant"],
			"settings": {
				"foreground": "#fbb",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["comment"],
			"settings": {
				"foreground": "#fff"
			}
		},
		{
			"scope": [
				"entity.name.type.alias",
				"entity.name.type.class",
				"entity.name.type.interface",
				"meta.definition.function"
			],
			"settings": {
				"foreground": "#f00",
				"fontStyle": "bold underline"
			}
		}
	]
}
